{

  "manifest_version": 2,
  "name": "thanos",
  "version": "1.0",
  "description": "When tiggered closes half of the opened tabs.",
  "author": "italogsfernandes",
  "icons": {
    "48": "icons/thanos-48.png",
    "128": "icons/thanos-128.png"
  },

  
  "permissions": [
    "bookmarks",
    "tabs"
  ],

  "browser_action": {
    "default_icon": "icons/thanos-48.png",
    "default_title": "Snap!"
  },

  "background": {
    "scripts": ["thanos.js"]
  }

}
